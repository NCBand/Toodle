<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>


    <g:HTMLPanel styleName='header'>
        <div class="logo">noodle</div>
        <div class="header_menu_item">Задания</div>
        <div class="header_menu_item">Оценки</div>
        <div class="header_menu_item">Сообщения</div>
        <g:MenuBar styleName="header_menu">
            <g:MenuItem>
                <span ui:field="userName">Андрей Мартыненко</span>
                <g:MenuBar vertical="true">
                    <g:MenuItem ui:field="homePage">Домашняя страница</g:MenuItem>
                    <g:MenuItem ui:field="profile">Профиль</g:MenuItem>
                    <g:MenuItem ui:field="settings">Настройки</g:MenuItem>
                    <g:MenuItem ui:field="signOut">Выход</g:MenuItem>
                </g:MenuBar>
            </g:MenuItem>
        </g:MenuBar>
    </g:HTMLPanel>

</ui:UiBinder>